{
  "mcpServers": {
    "context7": {
      "args": [
        "-y",
        "@upstash/context7-mcp"
      ],
      "autoApprove": [
        "resolve-library-id",
        "get-library-docs"
      ],
      "command": "npx",
      "env": {
        "DEFAULT_MINIMUM_TOKENS": "1000"
      }
    },
    "fetch": {
      "args": [
        "mcp-server-fetch"
      ],
      "autoApprove": [
        "fetch"
      ],
      "command": "uvx"
    },
    "sequentialthinking": {
      "args": [
        "-y",
        "@modelcontextprotocol/server-sequential-thinking"
      ],
      "autoApprove": [
        "sequentialthinking"
      ],
      "command": "npx"
    },
    "time": {
      "args": [
        "mcp-server-time"
      ],
      "autoApprove": [
        "get_current_time",
        "convert_time"
      ],
      "command": "uvx",
      "custom_instructions": {
        "text": ""
      }
    }
  }
}
