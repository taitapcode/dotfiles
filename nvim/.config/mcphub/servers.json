{
  "mcpServers": {
    "context7": {
      "args": [
        "-y",
        "@upstash/context7-mcp"
      ],
      "autoApprove": [
        "resolve-library-id",
        "get-library-docs"
      ],
      "command": "npx",
      "env": {
        "DEFAULT_MINIMUM_TOKENS": "1000"
      }
    },
    "git": {
      "args": [
        "mcp-server-git"
      ],
      "autoApprove": [
        "git_status",
        "git_diff_unstaged",
        "git_diff_staged",
        "git_diff",
        "git_commit",
        "git_add",
        "git_reset",
        "git_log",
        "git_create_branch",
        "git_checkout",
        "git_show",
        "git_branch"
      ],
      "command": "uvx"
    },
    "sequentialthinking": {
      "args": [
        "-y",
        "@modelcontextprotocol/server-sequential-thinking"
      ],
      "autoApprove": [
        "sequentialthinking"
      ],
      "command": "npx"
    },
    "time": {
      "args": [
        "mcp-server-time"
      ],
      "autoApprove": [
        "get_current_time",
        "convert_time"
      ],
      "command": "uvx",
      "custom_instructions": {
        "text": ""
      }
    }
  }
}
